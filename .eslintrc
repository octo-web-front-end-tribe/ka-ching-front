{
  "env": {
    "browser": true,
    "jasmine": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "inject": true,
    "L": true,
    "_": true,
    "$": true
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs"],
    "camelcase": [1, {"properties": "never"}],
    "comma-style": [2, "last"],
    "complexity": [1, 10],
    "handle-callback-err": 2,
    "indent": [2, 2],
    "max-depth": [1, 5],
    "max-len": [2, 120, 2],
    "new-cap": 1,
    "no-catch-shadow": 2,
    "no-console": 0,
    "no-continue": 1,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-extend-native": 1,
    "no-extra-parens": 1,
    "no-lonely-if": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-nested-ternary": 2,
    "no-path-concat": 2,
    "no-process-exit": 1,
    "no-proto": 1,
    "no-reserved-keys": 2,
    "no-self-compare": 2,
    "no-shadow": 1,
    "no-throw-literal": 2,
    "no-undefined": 1,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "no-void": 2,
    "no-wrap-func": 0,
    "one-var": [1, "never"],
    "operator-assignment": [2, "always"],
    "quote-props": [1, "as-needed"],
    "radix": 2,
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "space-in-brackets": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-line-comment": [1, "always"],
    "strict": [2, "never"]
  }
}